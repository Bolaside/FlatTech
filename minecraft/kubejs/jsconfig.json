{
    "compilerOptions": {
        "lib": [
            "ES5",
            "ES2015",
            "ES2017.Object"
        ],
        "rootDirs": [
            "probe/generated",
            "probe/user",
            "server_scripts",
            "startup_scripts",
            "client_scripts"
        ],
        "typeRoots": [
            "./probe/generated",
            "./probe/user"
        ],
        "skipLibCheck": false,
        "disableSizeLimit": true,
        "noEmit": true,
        "target": "ES2015"
    }
}